# properties.windows

swt.dir = swt-M20060629-1905-win32-win32-x86
swt.jni.dir = ${swt.dir}
swt.libs.dynamic = swt*.dll

install.home = blimp

dcraw.exe = blimp-dcraw.exe

# compile dcraw under cygwin without cygwin1.dll dependency
c-compiler-args = -mno-cygwin
c-linker-args = -mno-cygwin
dcraw.defines.minimal = NO_JPEG,NO_LCMS,W32_GETC_UNLOCKED
dcraw.libs.minimal = ws2_32
